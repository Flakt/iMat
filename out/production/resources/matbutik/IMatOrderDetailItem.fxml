<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<fx:root prefHeight="150.0" prefWidth="434.0" type="AnchorPane" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fx:id="orderDetailImage" fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fx:id="orderDetailEcoImage" fitHeight="25.0" fitWidth="50.0" layoutX="50.0" layoutY="63.0" pickOnBounds="true" preserveRatio="true" />
      <Label fx:id="orderDetailProductNameLabel" layoutX="119.0" layoutY="14.0" text="Label">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="orderDetailProductPriceLabel" layoutX="311.0" layoutY="14.0" text="Label">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="orderDetailProductAmountTextField" alignment="CENTER" editable="false" layoutX="285.0" layoutY="110.0" prefHeight="50.0" prefWidth="150.0" />
      <Button fx:id="orderDetailRemoveButton" layoutX="286.0" layoutY="110.0" mnemonicParsing="false" onAction="#removeButtonAction" prefHeight="50.0" prefWidth="50.0" text="-">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="orderDetailAddButton" layoutX="383.0" layoutY="110.0" mnemonicParsing="false" onAction="#addButtonAction" prefHeight="50.0" prefWidth="50.0" text="+">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</fx:root>
